<%= form_for @income do |f| %>
  <%= f.label :amount %>
  <%= f.text_field :amount %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :actual %>
  <%= f.text_field :actual %>
  <%= f.label :frequency %>
  <%= f.text_field :frequency %>
  <%= f.submit value="Add Entry"%>
<% end %>
