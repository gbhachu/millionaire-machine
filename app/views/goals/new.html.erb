<%= form_for @goal do |f| %>
  <%= f.label :amount %>
  <%= f.text_field :amount %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :start_date %>
  <%= f.text_field :start_date %>
  <%= f.label :target_date %>
  <%= f.text_field :target_date %>
  <%= f.submit 'Submit goal' %>
<% end %>
